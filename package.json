{
  "name": "design-history",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^16.0.0"
  },
  "description": "A place for documenting GOV.UK service designs",
  "keywords": [
    "design-history",
    "government",
    "govuk"
  ],
  "author": "Becoming a teacher team",
  "license": "MIT",
  "homepage": "https://design-history.herokuapp.com",
  "repository": "github:DFE-Digital/govuk-design-history",
  "bugs": "https://github.com/DFE-Digital/govuk-design-history/issues",
  "scripts": {
    "prebuild": "rimraf public",
    "build": "eleventy",
    "prestart": "npm run build",
    "start": "eleventy --serve --quiet",
    "test": "standard"
  },
  "dependencies": {
    "@11ty/eleventy": "^1.0.0-beta.7",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "accessible-autocomplete": "^2.0.3",
    "govuk-frontend": "^4.0.0",
    "http-server": "^14.0.0",
    "luxon": "^2.2.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-image-figures": "^2.0.0",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.0",
    "sass": "^1.45.2"
  },
  "devDependencies": {
    "standard": "^16.0.4",
    "webshot-node": "^0.18.2"
  },
  "standard": {
    "ignore": [
      "!.eleventy.js"
